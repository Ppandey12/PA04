<h1>Summary of all transaction</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>description</th>
            <th>amount</th>
            <th>category</th>
            <th>date</th>
        </tr>
    </thead>
    <tbody>
        <% results.map(item=> {%>
            <tr>
                <td>
                    <%= item.description %>
                </td>
                <td>
                    <%= item.amount%>
                </td>
                <td>
                    <%= item.category%>
                </td>
                <td>
                    <%= item.date %>
                </td>
            </tr>

            <% }) %>

    </tbody>
</table>